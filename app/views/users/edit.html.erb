<div class="row justify-content-center">
 <div class="col-md-6">
  <h2>プロフィール編集</h2>
   <%= form_with model: @user do |f| %>
    <label for="inputName">Name</label><br>
    <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%><br>
    <label for="inputImage">ProfileImage</label><br>


      <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %><br>
     </div>
    </div>
    <div class="row justify-content-center">
     <div class="coll-6">
      <%= f.submit "変更する" %>
     </div>
    </div>
   <% end %>
 </div>
</div>
